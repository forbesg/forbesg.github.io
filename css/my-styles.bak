/*body {
  background-color: #fff;
  color: #414141;
}
h1, h2, h3, h4 {
  color: #2b3e50;
  font-weight: lighter;
  text-transform: lowercase;
  margin: 30px 0;
  font-family: 'Quattrocento', serif;
}
p {
  color: #414141;
  line-height: 28px;
  margin: 0 0 30px;
}
a {
  color: #5650D3;
  text-decoration: none;
}
a:hover, a:focus {
  color: #21214B;
  text-decoration: none;
}
hr {
  margin-top: 21px;
  margin-bottom: 21px;
  border: 0;
  border-top: 1px solid #EAEAEA;
}
button, .button {
  background-color: transparent;
  border: 1px solid #2b3e50;
  margin: 20px 0;
  padding: 5px 10px;
  color: #2b3e50;
  transition: all 0.3s ease-in;
}
button:hover, .button:hover {
  background-color: #2b3e50;
  color: #fff;
  transition: all 0.3s ease-in;
}
.banner-text {
  padding: 150px 0;
  text-align: right;
}
h1.title {
  color: #fff;
  text-shadow: 1px 1px 0px #000;
  font-family: inherit;
  font-size: 24px;
  /*line-height: 450px;*/
  letter-spacing: 20px;
  text-transform: uppercase;
}
.banner-text p {
  font-size: 12px;
  color: #FFF670;
  letter-spacing: 5px;
  font-family: 'Quattrocento', serif;
  text-shadow: 1px 1px 0 #000;
  text-transform: lowercase;
  padding-right: 20px;
}
.nav-toggle {
  position: absolute;
  top: 10px;
  right: 10px;
  font-size: 30px;
  color: #fff;
  padding: 5px 10px;
  display: block;
  cursor: pointer;
  z-index: 3;
}
.nav-toggle:hover {
  color: #D5D5D5;
  text-shadow: 1px 1px 0 #585858;
}
.nav-toggle i {
  transition: all 0.3s ease;
}
header nav {
  display: none;
  position: fixed;
  z-index: 2;
  top: 0;
  right: 0;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 100%;
  text-align: center;
  padding-top: 50px;
  background: rgba(0, 0, 0, 0.85);
}
header nav ul {
  padding-left: 0;
  line-height: 50px;
  margin-bottom: 0;
  height: 100%;
}
header nav ul li {
  display: block;
  height: 20%;
}
header nav ul li a {
  color: #FFF;
  font-size: 16px;
  text-transform: uppercase;
  display: block;
  padding: 10px;
  text-shadow: 1px 1px 0 #000;
}
header nav ul li a:hover {
  color: #E2E2E2;
  text-decoration: none;
}
.logo img {
  height: 50px;
  margin: 20px;
  transition: all 0.2s ease-in;
}
.logo img:hover {
  background-color: #2b3e50;
  padding: 2px;
}
.banner {
  background-color: #2b3e50;
  background-image: url(../images/background.jpg);
  height: 600px;
  background-position: center;
  background-size: cover;
  border-bottom: 1px solid #fff;
  color: #000;
  overflow: hidden;
}
.banner.home {
  background-image: url(../images/background.jpg);
}
.banner.about {
  background-image: url(../images/about-bg.jpg);
}
.banner.contact {
  background-image: url(../images/contact-bg.jpg);
}
.banner.portfolio {
  background-image: url(../images/portfolio-bg.jpg);
}
.banner.project-page {
  background-image: url(../images/project-bg.jpg);
}
.top-bottom-margin {
  margin-top: 20px;
  margin-bottom: 50px;
}
.page-contents img {
  width: 100%;
}
.project-container {
  position: relative;
  overflow: hidden;
}
.project {
  margin: 60px 0;
}
.project a {
  /*display: block;*/
  -webkit-filter: grayscale(50%);
  filter: grayscale(50%);
  transition: all 0.5s ease-in-out;
}
.project a:hover {
  text-decoration: none;
  -webkit-filter: grayscale(0%);
  filter: grayscale(0%);
  transition: all 0.5s ease-in-out;
}
.project img {
  max-width: 100%;
}
.overlay-block {
  position: absolute;
  background-color: rgba(255, 255, 255, 0.8);
  top: 0;
  width: 100%;
  height: 100%;
  padding: 20% 0;
  display: block;
  transition: all 0.3s cubic-bezier(0.300, 0.100, 0.580, 1.000);
}
.overlay-block button{
  background-color: transparent;
  color: #000;
  border: 1px solid #000;
}
.overlay-block button:hover  {
  background-color: #2b3e50;
  color: #fff;
  border: 1px solid #2b3e50;
  transition: all 0.3s cubic-bezier(0.300, 0.100, 0.580, 1.000);
}
.text-block {
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
}
.card {
  padding: 20px;
}
.card img {
  margin-bottom: 20px;
  border: 1px solid #EEE;
}
.card .text-center {
  position: absolute;
  bottom: -30px;
  right: 20px;
}
.google-card {
  width: 250px;
  padding: 0 20px 20px 20px;
  margin: auto;
}

/*** Contact Form Styling ***/

.form-control {
  border: none;
  -webkit-box-shadow: none;
  border: 1px solid #A6A6A6;
}
.form-control:focus {
  border-color: #0A2E52;
}
.social-links {
  margin-bottom: 20px;
}
.social-contacts i, .contacts i {
  font-size: 20px;
  width: 40px;
}
footer {
  background-color: #2b3e50;
  color: #000;
  margin-top: 50px;
  padding: 20px 0;
}
footer ul {
  padding-left: 0;
  margin: 20px;
}
footer nav li {
  color: #000;
  display: inline-block;
}
footer nav li a {
  padding: 5px 10px;
  display: inline-block;
  text-transform: uppercase;
  font-size: 12px;
  color: #fff;
}
footer nav li a:hover {
  color: #E2E2E2;
  text-decoration: none;
}
.social {
  padding: 30px 0;
}
footer a i.fa {
  color: #fff;
  font-size: 20px;
  margin: 0 20px;
  transition: all 0.2s ease;
}
footer a i.fa:hover {
  color: #000;
  transform: scale(1.2);
  transition: all 0.2s ease;
}
p.copy {
  color: #dadada;
  font-size: 12px;
}

/*** 404 Page ***/

h1.four-o-four {
  display:inline-block;
  animation-delay: 2s;
}
.not-found h1 {
  font-size: 80px;
  border: 1px solid #2b3e50;
  /* width: auto; */
  display: inline-block;
  border-radius: 50%;
  padding: 50px;
}

/*** Media Queries ***/


/* Small devices (tablets, 768px and up) */

@media (min-width: 768px) {
  h1.title {
    font-size: 34px;
  }
  .banner-text p {
    font-size: 16px;
  }
  footer nav li a {
    font-size: 16px;
  }
  .not-found h1 {
    font-size: 160px;
  }
}

/* Medium devices (desktops, 992px and up) */

@media (min-width: 992px) {
  header nav {
    display: block;
    float: right;
    position: relative;
    width: auto;
    height: auto;
    padding: 0;
    text-align: center;
    background: transparent;
  }
  .nav-toggle {
    display: none;
  }
  header nav ul {
    padding-left: 0;
    line-height: 50px;
    margin-bottom: 0;
  }
  header nav ul li {
    display: inline-block;
  }
  header nav ul li a {
    font-size: 14px;
    display: inline-block;
  }
  header nav ul li a:hover {
    color: #E2E2E2;
    text-decoration: none;
  }
  .banner-text {
    padding: 180px 0;
  }
  .page-contents img {
    width: 70%;
    float: right;
    margin-left: 20px;
  }
  .project {
    margin: 20px 0;
  }
  .overlay-block {
    top: 500px;
  }
  .overlay-block.button-hover {
    background-color: rgba(255, 255, 255, 0.8);
    top: 0;
  }
}

/* Large devices (large desktops, 1200px and up) */

@media (min-width: 1200px) {
  .page-contents img {
    width: auto;
    float: right;
    margin-left: 20px;
  }
}*/
